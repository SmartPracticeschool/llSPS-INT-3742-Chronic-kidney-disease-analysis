        "#split the data into independent (x) data set and dependent (y) data set (the target)\n",
        "X = df.drop(['classification'], axis=1)\n",
        "Y = df['classification']"
      ],
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "273ja9rhzRQb",
